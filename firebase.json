{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/HangmansHollow/**",
        "destination": "/HangmansHollow/index.html"
      },
      {
        "source": "/void/**",
        "destination": "/void/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}